#include<stdio.h>
int main(){
	int x, y, t;
	printf("Enter first number x : ");
	scanf("%d", &x);
	printf("Enter second number y : ");
	scanf("%d", &y);
	//x value is assigned to temp
	x = t;
	//y is assigned to x
	y = x;
	//t is assigned to y
	t = y;	
	printf("\n after swapping x : %d\n", x);
	printf("\n after swapping y : %d\n", y);
	return 0;
	
}
